{
  "name": "template",
  "private": true,
  "version": "0.0.0",
  "description": "Electron + Vue3 App Template",
  "author": "Joseph Liang",
  "main": "electron/main.js",
  "scripts": {
    "web": "vite",
    "build": "vite build && electron-builder",
    "preview": "vite preview",
    "dev": "node server/dev.js"
  },
  "dependencies": {
    "@types/node": "^20.2.5",
    "axios": "^1.4.0",
    "electron-updater": "^6.2.1",
    "pinia": "^2.1.7",
    "terser": "^5.18.2",
    "vue": "^3.4.29",
    "vue-router": "^4.3.3"
  },
  "devDependencies": {
    "@iconify/vue": "^4.1.1",
    "@iconify/json": "^2.2.163",
    "@unocss/preset-attributify": "^0.57.2",
    "@unocss/preset-uno": "^0.57.2",
    "@vitejs/plugin-vue": "^5.0.5",
    "naive-ui": "^2.35.0",
    "concurrently": "^8.2.2",
    "electron": "^31.1.0",
    "electron-builder": "^24.13.3",
    "sass": "^1.62.1",
    "unocss": "^0.57.2",
    "rimraf": "^5.0.5",
    "rollup-plugin-visualizer": "^5.6.0",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-icons": "^0.17.1",
    "unplugin-vue-components": "^0.25.1",
    "vfonts": "^0.0.3",
    "vite": "^4.3.9",
    "vite-plugin-compression": "^0.5.1"
  },
  "build": {
    "appId": "mockId",
    "productName": "Electron + Vue3 App Template",
    "copyright": "Copyright Â© year ${author}",
    "directories": {
      "output": "app_client"
    },
    "publish": {
      "provider": "generic",
      "url": "https://example.com/app_client/",
      "channel": "latest"
    },
    "mac": {
      "category": "public.app-category.music",
      "icon": "public/icon.icns",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "win": {
      "icon": "public/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "public/icon.ico",
      "uninstallerIcon": "public/icon.ico"
    }
  }
}
